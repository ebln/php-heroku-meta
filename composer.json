{
    "name": "brnc/php-heroku-meta",
    "type": "metapackage",
    "description": "Helper to get sync Heroku's available extentions.",
    "authors": [
        {
            "name": "ebln",
            "email": "34722048+ebln@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mysqlnd": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-phar": "*",
        "ext-posix": "*",
        "ext-readline": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-simplexml": "*",
        "ext-spl": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zlib": "*"
    },
    "conflict": {
        "ext-mcrypt": "*",
        "ext-xmlrpc": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.15",
        "roave/security-advisories": "dev-latest"
    },
    "suggest": {
        "ext-ftp": "Available by Heroku when required by composer.json",
        "ext-mbstring": "Available by Heroku when required by composer.json",
        "ext-pdo_sqlite": "Available by Heroku when required by composer.json",
        "ext-sodium": "Available by Heroku when required by composer.json",
        "ext-sqlite3": "Available by Heroku when required by composer.json"
    }
}
